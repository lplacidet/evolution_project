    "        if(evaluate_bird(c.x) > fitness_cumu)\n",
    "            fitness_cumu = evaluate_bird(c.x)\n",
    "        end\n",
    "        dist[i] = fitness_cumu\n",
    "    end\n",
    "    [c.x, sigs, dist]\n",
