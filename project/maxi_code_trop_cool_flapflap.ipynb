    "    \n",
    "    dist = zeros(Nstep)\n",
    "    for i in 1:Nstep\n",
    "        es_step!(obj, npop=Npop, sigma=Sigma)\n",
    "        #println(\"best: \", best)\n",
    "        #println(\"best norm: \", norm(best))\n",
    "        #println(\"dist max :\" , objective(best))\n",
    "        dist[i] = objective(obj.center.genes)\n",
    "        #if dist[i]>300\n",
    "         #   Sigma = 0.01\n",
    "        #end\n",
